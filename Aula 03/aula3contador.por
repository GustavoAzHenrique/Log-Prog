programa
{
	
	funcao inicio()
	{
		inteiro i=0
		enquanto(i <= 100){
			escreva(i,"\n")
			i = i + 1
		}
		escreva("\nTerminou\n")

		inteiro j=100
		enquanto(j >= 0){
			escreva(j,"\n")
			j = j - 1
		}
	}
}
